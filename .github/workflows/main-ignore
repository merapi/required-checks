name: CI

on:
  push:
    branches: [ "master" ]
    paths-ignore:
      - '**.tsx'
      - '**.ts'
  pull_request:
    branches: [ "master" ]
    paths-ignore:
      - '**.tsx'
      - '**.ts'

  workflow_dispatch:

jobs:
  test:
    runs-on: ubuntu-latest
    steps:
      - run: 'echo "Dummy required check"'